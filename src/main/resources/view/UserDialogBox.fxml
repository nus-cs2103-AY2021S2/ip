<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
    <VBox alignment="TOP_RIGHT" prefWidth="279.0"
          style="-fx-background-color: #FAEDBE#FAEDBE #FAEDBE#FAEDBE;">
        <Label fx:id="nameLabel" text="Name" textFill="#e4b612" wrapText="true">
            <padding>
                <Insets top="10" right="15" left="15"/>
            </padding>
            <font>
                <Font name="Avenir Next Bold" size="15.0"/>
            </font>
        </Label>
        <Label fx:id="dialog" text="Label" textFill="#81735e" wrapText="true">
            <padding>
                <Insets top="5" right="15" bottom="5" left="15"/>
            </padding>
        </Label>
    </VBox>
    <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
</fx:root>
