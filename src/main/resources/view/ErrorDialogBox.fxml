<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: white;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <VBox maxHeight="1.7976931348623157E308" minHeight="-Infinity" style="-fx-background-radius: 5; -fx-background-color: #f3eded #f3eded; -fx-border-radius: 1; -fx-border-color: red; -fx-border-style: dashed; -fx-border-radius: 5;" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="senderName" text="Label" wrapText="true">
               <padding>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Label>
          <Label fx:id="dialog" maxHeight="1.7976931348623157E308" minHeight="-Infinity" text="Label" textOverrun="CLIP" wrapText="true" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Label>
         </children>
      </VBox>
    <ImageView fx:id="displayPicture" fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" style="-fx-border-radius: 50%;">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin></ImageView>
  </children>
  <padding>
    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
  </padding>
</fx:root>
