<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="duke.ui.MainMenu">
   <children>
      <VBox fx:id="logoBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: grey; -fx-border-color: white; -fx-border-width: 6;">
         <children>
            <Label fx:id="dukeLogo" alignment="CENTER" contentDisplay="CENTER" prefHeight="360.0" prefWidth="600.0" style="-fx-background-color: grey; -fx-border-color: white;" text="Label" textFill="WHITE">
               <font>
                  <Font size="94.0" />
               </font>
            </Label>
            <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#switchWindow" prefHeight="39.0" prefWidth="110.0" style="-fx-border-color: grey;" text="Start" textAlignment="CENTER" textFill="#4261bc" translateX="240.0" underline="true">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
