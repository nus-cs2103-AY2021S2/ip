<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT" maxWidth="400.0" minHeight="-Infinity" spacing="1.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="dialog" maxWidth="280.0" minHeight="-Infinity" style="-fx-background-color: #ffffff; -fx-background-radius: 15;" text="Label" wrapText="true">
         <HBox.margin>
            <Insets bottom="12.0" left="5.0" right="5.0" top="12.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
    <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin></ImageView>
  </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</fx:root>
