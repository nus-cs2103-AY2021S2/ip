<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER_RIGHT" maxHeight="Infinity" maxWidth="Infinity" prefWidth="400.0" spacing="20.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" contentDisplay="CENTER" lineSpacing="5.0" style="-fx-background-color: #add8e6;" text="Label" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Garamond" size="16.0" />
            </font>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding></Label>
        <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets bottom="10.0" right="5.0" top="10.0" />
            </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>