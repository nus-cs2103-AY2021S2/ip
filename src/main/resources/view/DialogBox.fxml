<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" type="HBox"
         xmlns="http://javafx.com/javafx/8.0.171">
    <children>
        <javafx.scene.layout.HBox alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308"
                                  maxWidth="1.7976931348623157E308" prefWidth="400.0" spacing="10.0"
                                  style="-fx-border-color: #000000; -fx-border-radius: 15px; -fx-background-color: black; -fx-background-radius: 15px;">
            <children>
                <Label fx:id="dialog" maxHeight="1.7976931348623157E308" minHeight="-Infinity" prefWidth="388.0"
                       text="Label" textFill="#eb79da" wrapText="true">
                    <font>
                        <Font name="Microsoft Sans Serif" size="20.0"/>
                    </font>
                </Label>
            </children>
            <padding>
                <Insets bottom="0.0" left="10.0" right="0.0" top="0.0"/>
            </padding>
            <cursor>
                <Cursor fx:constant="NONE"/>
            </cursor>
        </javafx.scene.layout.HBox>
        <ImageView fx:id="displayPicture" fitHeight="115.0" fitWidth="115.0" pickOnBounds="true">
            <image>
                <Image url="@../images/DanhDuke.png"/>
            </image>
            <HBox.margin>
                <Insets left="5.0" right="5.0"/>
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
